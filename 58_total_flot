# Tuple of float values
values = (10.5, 20.0, 30.25, 40.75)

total = 0.0
count = 0

# Calculate sum using for loop
for v in values:
    total = total + v
    count = count + 1

# Calculate average
average = total / count

print("Tuple values:", values)
print("Sum of float values:", total)
print("Average of float values:", average)
